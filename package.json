{
  "name": "defi.js.org",
  "private": true,
  "version": "2.0.0",
  "description": "Defi.js website",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist html/*/temp",
    "test": "echo 'No test specified'",
    "build": "webpack --config webpack/webpack.config.js",
    "watch": "PORT=8110 NODE_ENV=development npm run build -- --watch",
    "serve-dist": "http-server dist -o -s -d -p 8110",
    "deploy": "npm run clear && deploy-to-git",
    "dev": "npm-run-all --parallel watch serve-dist",
    "upgrade": "ncu -u && npm i",
    "reinstall": "rm -rf node_modules package-lock.json && npm i"
  },
  "config": {
    "deployToGit": {
      "repository": "https://$GH_TOKEN@github.com/defijs/defi.js.org.git",
      "branch": "gh-pages",
      "folder": "dist",
      "script": "npm run build",
      "commit": "Deployment",
      "user": {
        "email": "andrey@gubanov.eu",
        "name": "Andrey Gubanov"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "compile-ejs-loader": "0.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "deploy-to-git": "^0.1.5",
    "ejs": "^3.1.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "glob": "^7.1.6",
    "html-loader": "^1.1.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "jsdoc": "^3.6.4",
    "json-loader": "^0.5.7",
    "marked": "^1.0.0",
    "node-sass": "^4.14.1",
    "node-sass-asset-functions": "0.1.0",
    "npm-check-updates": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "string-replace-loader": "^2.3.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^1.2.1",
    "uglify-js": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yaml-loader": "^0.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "balajs": "^1.0.7",
    "es6-promise": "^4.2.8",
    "github-embed": "^1.0.1",
    "highlight.js": "^9.15.10",
    "isomorphic-fetch": "^2.2.1",
    "markdown-loader": "^5.1.0",
    "matreshka": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matreshkajs/matreshka.io.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/matreshkajs/matreshka.io/issues"
  },
  "homepage": "https://github.com/matreshkajs/matreshka.io#readme"
}
